<templateSet group="user">
  <template name="ajs" value="alert(JSON.stringify($object$));" description="alert显示对象的JSON字符串" toReformat="true" toShortenFQNames="true">
    <variable name="object" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ift" value="&lt;if test=&quot;$param$ != null and $param$ != ''&quot;&gt;&#10;    $END$&#10;&lt;/if&gt;" description="mybatis-测试参数是否为空" toReformat="true" toShortenFQNames="true">
    <variable name="param" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="SQL" value="true" />
      <option name="XML_TEXT" value="true" />
    </context>
  </template>
  <template name="dlog" value="private org.apache.log4j.Logger logger = org.apache.log4j.Logger.getLogger(this.getClass());" description="声明logger对象" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="qui:box2" value="&lt;div class=&quot;box2&quot; showStatus=&quot;true&quot; startState=&quot;open&quot; statusText=&quot;打开&quot; afterStatusText=&quot;隐藏&quot; panelTitle=&quot;查询&quot;&gt; &#10;    $END$&#10;&lt;/div&gt;" description="插入QUI的box2模型" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="a" value="alert($object$);" description="alert" toReformat="true" toShortenFQNames="true">
    <variable name="object" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sie" value="org.apache.commons.lang3.StringUtils.isEmpty($str$)" description="Check String is null or size zero" toReformat="true" toShortenFQNames="true">
    <variable name="str" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="map" value="java.util.Map&lt;java.lang.String, java.lang.Object&gt; $param$ = new java.util.HashMap&lt;java.lang.String, java.lang.Object&gt;();" description="Map&lt;String,Object&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="param" expression="" defaultValue="param" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="user" value="com.fencer.wiciot.system.bean.UserBean user = (com.fencer.wiciot.system.bean.UserBean)this.getSession().getAttribute(com.fencer.wiciot.util.SystemCons.SESSION_LOGINED_USER);" description="从Session获取UserBean" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="tiae" value="throw new java.lang.IllegalArgumentException($msg$);" description="抛出IAE" toReformat="true" toShortenFQNames="true">
    <variable name="msg" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="set" value="$col$ = #{$col$},$END$" description="设置字段" toReformat="true" toShortenFQNames="false">
    <variable name="col" expression="" defaultValue="&quot;col&quot;" alwaysStopAt="true" />
    <context>
      <option name="SQL" value="true" />
    </context>
  </template>
  <template name="sne" value="org.apache.commons.lang3.StringUtils.isNotEmpty($str$)" description="Check String Not Empty" toReformat="true" toShortenFQNames="true">
    <variable name="str" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
</templateSet>